<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.flagtag.wrinkle">


    <!-- 인터넷 권한 선언 -->
    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.flagtag.wrinkle.MainActivity"/>
        <activity android:name="com.flagtag.wrinkle.googleLoginActivity">
            <!--  인텐트 필터로 첫 실행 액티비티 선정 -->
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />


                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.flagtag.wrinkle.LoginActivity"/>
        <activity android:name="com.flagtag.wrinkle.PasswordResetActivity"/>
        <activity android:name="com.flagtag.wrinkle.MemberActivity"/>
        <activity android:name=".RegisterActivity"/>



    </application>

</manifest>
